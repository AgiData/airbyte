{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Event Schema",
    "type": "object",
    "properties": {
        "id": {"type": "string"},
        "deleted": {"type": ["null", "string"]},
        "deleted_by_cascade": {"type": "boolean"},
        "created_at": {"type": "string", "format": "date-time"},
        "updated_at": {"type": "string", "format": "date-time"},
        "rank": {"type": "integer"},
        "title": {"type": "string"},
        "description": {"type": ["null", "string"]},
        "recording": {"type": ["null", "object"]},
        "capacity": {"type": "integer"},
        "sponsor_image": {"type": "string"},
        "tags": {
            "type": ["array", "null"],
            "items": {"type": "string"}
        },
        "mmid": {"type": "string"},
        "shadow_mmid": {"type": ["null", "string"]},
        "starts_at": {"type": ["null", "string"]},
        "ends_at": {"type": ["null", "string"]},
        "is_live": {"type": "boolean"},
        "is_booth": {"type": "boolean"},
        "is_breakout": {"type": "boolean"},
        "current": {"type": "boolean"},
        "video_quality": {"type": ["null", "string"]},
        "event": {"type": "string"},
        "owner": {"type": ["null", "object"]}
    },
    "required": [
        "id",
        "deleted",
        "deleted_by_cascade",
        "created_at",
        "updated_at",
        "rank",
        "title",
        "description",
        "recording",
        "capacity",
        "sponsor_image",
        "tags",
        "mmid",
        "shadow_mmid",
        "starts_at",
        "ends_at",
        "is_live",
        "is_booth",
        "is_breakout",
        "current",
        "video_quality",
        "event",
        "owner"
    ]
}
