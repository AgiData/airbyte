{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Tracks Schema",
  "type": "object",
  "properties": {
    "id": { "type": "string" },
    "allowed_ticket_types": { "type": "array", "items": { "type": "string" } },
    "agenda_items": { "type": "string" },
    "created_at": { "type": "string", "format": "date-time" },
    "updated_at": { "type": "string", "format": "date-time" },
    "custom": { "type": "boolean" },
    "title": { "type": "string" },
    "channel_mmid": { "type": ["null", "string"] },
    "sort_priority": { "type": "integer" },
    "event": { "type": "string" }
  },
  "required": [
    "id",
    "allowed_ticket_types",
    "agenda_items",
    "created_at",
    "updated_at",
    "custom",
    "title",
    "sort_priority",
    "event"
  ]
}
